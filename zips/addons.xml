        <import addon="xbmc.python" version="2.14.0" />
    </requires>
    <extension point="xbmc.python.script" library="default.py" />
    <extension point="xbmc.addon.metadata">
        <summary lang="en">Syncs stuff from smashing to and from userdata.  Hopefully.</summary>
        <platform>all</platform>
    </extension>
</addon>

<addon id="service.subtitles.subscene.foreignonly"
       name="Subscene.com (ForeignOnly - my version)"
       version="1.7.2.7"
       provider-name="CrowleyAJ, wuff, thuongtin.com">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
    </requires>
    <extension point="xbmc.subtitle.module"
               library="service.py"/>
    <extension point="xbmc.addon.metadata">
        <summary lang="en">Get subtitles for the foreign parts only from Subscene.com</summary>
        <description lang="en">Fork of thuongtin's (forked) subscene subtitle addon. The only change is to exclusively show subs for foreign parts only. </description>
        <disclaimer lang="en"/>
        <platform>all</platform>
        <language/>
        <license>GNU GENERAL PUBLIC LICENSE. Version 2, June 1991</license>
        <forum>http://forum.xbmc.org/showthread.php?tid=184854</forum>
        <website/>
        <email/>
        <source>https://github.com/manacker/service.subtitles.subscene</source>
    </extension>
</addon>
</addons>
