             library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Sports Streaming</summary>
    <description lang="en">Streaming video plugin</description>
    <platform>all</platform>
    <license>GPL 2.0</license>
  </extension>
  </addon>

